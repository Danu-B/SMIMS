<div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
        <h1 class="text-center">Tenant Registratioin</h1>

        <form class="form-inline" action="tenantRegistration" method="POST">
            <form class="needs-validation" novalidate>
                <label class="lable col-md-3 control-lable">Full Name </label>
                <div class="col-md-9">
                    <input id="fullName" type="text" class="form-control" name="fullname" placeholder="name">
                </div>
                <label class="lable col-md-3 control-lable">Company name </label>
                <div class="col-md-9">
                    <input type="text" class="form-control" name="compnayName" placeholder="Company Name">
                </div>
                <label class="lable col-md-3 control-lable">Phone number </label>
                <div class="col-md-9">
                    <input type="number" class="form-control" name="phonenumber" placeholder="phone number">
                </div>

                <label class="lable col-md-3 control-lable">Tin number</label>
                <div class="col-md-9">
                    <input type="number" class="form-control" name="tinNumber" placeholder="Tin number">
                </div>
                <label class="lable col-md-3 control-lable">Address</label>
                <div class="col-md-9">
                    <input type="number" class="form-control" name="address" placeholder="Address">
                </div>
                <label class="lable col-md-3 control-lable">User Name</label>
                <div class="col-md-9">
                    <input type="text" class="form-control" name="userName" placeholder="name">
                </div>

                <label class="lable col-md-3 control-lable">Password</label>
                <div class="col-md-9">
                    <input type="Password" class="form-control" name="password" placeholder="Password">
                </div>
                <button type="submit" class="btn btn-info">Submit</button>
                {{!-- <a href="tenantRegistration">
                    <div class="btn btn-info">Submit</div>
                </a> --}}
            </form>
        </form>
    </div>
    <div class="col-md-3"></div>
</div>
</div>